			<label>
				<input type="password" name="password" required placeholder="Password">
			</label>
			<input type="submit" value="Login">
		</form>
		<h5><a href="#">Forgot password</a></h5>
	</section>
	<div style="text-align:center;clear:both">
 
</div>
</body>
</html>

